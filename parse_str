#include "main.h"
/**
 * parse_str - count str
 * @arg: string
 * @count: counter
 * Return: count
 */
int parse_str(va_list arg, int count)
{
char *str;
int i = 0;
str = va_arg(arg, char *);
if (str == NULL)
str = "(null)";
if (str[0] == '\0')
count++;
while (str[i] != '\0')
{
i++;
count++;
}
return (count);
}
